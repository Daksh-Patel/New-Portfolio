{
  "name": "daksh-new-portfolio",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "start": "set port=1520 && react-scripts start",
    "build": "react-scripts build",
    "clean": "rm -rf build && rm -rf node_modules",
    "test": "jest --maxWorkers=50% --passWithNoTests",
    "test:watch": "jest --maxWorkers=25% --watch --onlyChanged ",
    "test:ci": "jest --color --runInBand --ci",
    "eject": "react-scripts eject",
    "format": "prettier --write .",
    "check:format": "prettier --check .",
    "check:lint": "eslint . --ext .ts,.tsx --config .eslintrc",
    "check:types": "tsc --pretty --noEmit",
    "check:all": "yarn check:format && yarn check:lint && yarn check:types && yarn build",
    "prepare": "husky install"
  },
  "dependencies": {
    "@chakra-ui/react": "^2.4.3",
    "@chakra-ui/skip-nav": "^2.0.12",
    "@chakra-ui/theme-tools": "^2.0.14",
    "@chakra-ui/utils": "^2.0.12",
    "@emotion/react": "^11.10.5",
    "@emotion/styled": "^11.10.5",
    "@testing-library/jest-dom": "^5.16.5",
    "@testing-library/react": "^13.4.0",
    "@testing-library/user-event": "^14.4.3",
    "@types/jest": "^28.1.8",
    "@types/node": "^12.20.55",
    "@types/react": "^18.0.26",
    "@types/react-dom": "^18.0.9",
    "framer-motion": "^6.5.1",
    "husky": "^8.0.2",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-icons": "^3.11.0",
    "react-scripts": "5.0.1",
    "typescript": "^4.9.4",
    "web-vitals": "^2.1.4"
  },
  "devDependencies": {
    "prettier": "^2.8.1"
  },
  "lint-staged": {
    "**/*": [
      "prettier --write"
    ]
  },
  "eslintConfig": {
    "overrides": [
      {
        "files": [
          "**/*.stories.*"
        ],
        "rules": {
          "import/no-anonymous-default-export": "off"
        }
      }
    ]
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  }
}
